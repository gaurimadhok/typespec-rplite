    },
    "files": [
      {
        "path": "package.json",
        "destination": "package.json",
        "skipGeneration": true
      },
      {
        "path": "./arm-alltypes/main.tsp",
        "destination": "main.tsp"
      },
      {
        "path": "./arm-alltypes/locationbasedemployee.tsp",
        "destination": "locationbasedemployee.tsp"
      },
      {
        "path": "./arm-alltypes/proxyemployee.tsp",
        "destination": "proxyemployee.tsp"
      },
      {
        "path": "./arm-alltypes/tenantproxyemployee.tsp",
        "destination": "tenantproxyemployee.tsp"
      },
      {
        "path": "./arm-alltypes/extensionemployee.tsp",
        "destination": "extensionemployee.tsp"
      },
      {
        "path": "./arm-alltypes/tspconfig.yaml",
        "destination": "tspconfig.yaml"
      },
      {
        "path": "./arm-alltypes/examples/2021-10-01-preview/Operations_List.json",
        "destination": "examples/2021-10-01-preview/Operations_List.json"
      },
      {
        "path": "./arm-alltypes/examples/2021-10-01-preview/Employees_Get.json",
        "destination": "examples/2021-10-01-preview/Employees_Get.json"
      },
      {
        "path": "./arm-alltypes/examples/2021-10-01-preview/Employees_ListByResourceGroup.json",
        "destination": "examples/2021-10-01-preview/Employees_ListByResourceGroup.json"
      },
      {
        "path": "./arm-alltypes/examples/2021-10-01-preview/Employees_ListBySubscription.json",
        "destination": "examples/2021-10-01-preview/Employees_ListBySubscription.json"
      }
    ],
    "inputs": {
      "ServiceNamespace": {
        "description": "Please provide ARM Resource Provider Name in Pascal case:",
        "type": "text",
        "initialValue": "Microsoft.Contoso"
      },
      "APIVersion": {
        "description": "Please provide API version:",
        "type": "text",
        "initialValue": "2021-10-01-preview"
      }
    }
  }
}

{
  "azure-arm-operations": {
    "title": "(rest-api-spec repo) Azure Resource Manager Service Project with only Operations Resource Type",
    "description": "Create a project with only operations resource type in rest-api-spec repo, representing an Azure service ARM API",
    "compilerVersion": "1.0.0-rc.0",
    "libraries": [],
    "skipCompilerPackage": true,
    "config": {
      "emit": ["@azure-tools/typespec-autorest"]
    },
    "files": [
      {
        "path": "package.json",
        "destination": "package.json",
        "skipGeneration": true
      },
      {
        "path": "./arm-operations/main.tsp",
        "destination": "main.tsp"
      },
      {
        "path": "./arm-operations/operation.tsp",
        "destination": "operation.tsp"
      },
      {
        "path": "./common/tspconfig.yaml",
        "destination": "tspconfig.yaml"
      },
      {
        "path": "./arm-operations/examples/2021-10-01-preview/Operations_List.json",
        "destination": "examples/{{parameters.APIVersion}}/Operations_List.json"
      },
      {
        "path": "./common/readme.md",
        "destination": "../resource-manager/readme.md"
      }
    ],
    "inputs": {
      "ServiceNamespace": {
        "description": "Please provide ARM Resource Provider Name in Pascal case:",
        "type": "text",
        "initialValue": "Microsoft.Contoso"
      },
      "APIVersion": {
        "description": "Please provide API version:",
        "type": "text",
        "initialValue": "2021-10-01-preview"
      }
    }
  },
